{
  "name": "bsky staging hack",
  "description": "Changes bsky.app -> staging.bsky.app",
  "author": "jeff@techno.email",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "webNavigation"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "icons": {
    "128": "logo128.png"
  }
}